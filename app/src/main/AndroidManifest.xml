            <intent-filter>
                <action android:name="android.intent.action.EDITOR" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
  
        <activity
            android:name="com.surveyorexpert.Email"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.EMAIL" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>   
        
          <activity
            android:name="com.surveyorexpert.Register"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.REGISTR" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>    
        
      <activity
            android:name="com.surveyorexpert.ExplorerActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.EXPLORER" />
