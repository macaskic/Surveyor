        </activity>     
        
        <activity
            android:name="com.surveyorexpert.UploadImage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.UPLOADIMAGE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.surveyorexpert.DetailListView"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.DETAILLISTVIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>    
      
        <activity
            android:name="com.surveyorexpert.AdminIntroduction"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.ADMININTRODUCTION" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>     
            
